        "./influxdb-client/packages/core/src/impl/Logger.ts",
        "./influxdb-client/packages/core/src/impl/ObservableQuery.ts",
        "./influxdb-client/packages/core/src/impl/RetryBuffer.ts",
        "./influxdb-client/packages/core/src/impl/retryStrategy.ts",
        "./influxdb-client/packages/core/src/impl/QueryApiImpl.ts",
        "./influxdb-client/packages/core/src/impl/WriteApiImpl.ts",
        "./influxdb-client/packages/core/src/impl/node/nodeChunkCombiner.ts",
